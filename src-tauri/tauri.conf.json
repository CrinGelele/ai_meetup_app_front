{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "aima",
  "version": "0.1.0",
  "identifier": "app.aima.love",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "https://192.168.1.79:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "aima",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "http": {}
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
